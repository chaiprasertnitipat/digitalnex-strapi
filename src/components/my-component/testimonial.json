    },
    "listing_column_mobile": {
      "type": "integer",
      "max": 10,
      "min": 0,
      "default": 0
    },
    "listing_slide_desktop": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "listing_slide_mobile": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "title": {
      "type": "component",
      "repeatable": false,
